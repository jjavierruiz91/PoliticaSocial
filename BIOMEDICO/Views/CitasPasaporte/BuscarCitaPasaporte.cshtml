            <input style="border: black; border-width: 1px; border-style: solid;" class="form-control" id="CorreoPasaporte" name="CorreoPasaporte" disabled />
        </div>

        <script>
            function toggleButton() {
                var TipoIdentificacion = document.getElementById('TipoIdentificacion').value;
               /* var NumDocumentoPasaporte = document.getElementById('NumDocumentoPasaporte').value;*/
                if (TipoIdentificacion) {
                    document.getElementById('boton').disabled = false;
                } else {
                    document.getElementById('boton').disabled = true;
                }
            }
        </script>

        <br />

        <div class="col-xs-3 col-sm-3 col-md-3" style="right:inherit">

