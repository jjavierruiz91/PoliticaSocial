<h1 class="p-1 mb-2 text-white" style="background-color: #006068; text-align: center; "><strong>SOLICITUD DE TRAMITE PASAPORTE</strong></h1>
<hr width=100% align="right" size=200 color="black">

<div class="">

    <form id="FormCitasPasaporte" name="FormCitasPasaporte">

        <div class="row m-1">


            <div class="col-xs-12 col-sm-12 col-md-12">
                <label><strong>Tipo Documento</strong> </label>
                <select style="border: black; border-width: 1px; border-style: solid;" class="select2 form-control" id="TipoIdentificacion" name="TipoIdentificacion" onchange="toggleButton()" >
                    <option value="">Seleccionar</option>
                    <option value="Cedula de Ciudadania">Cedula de Ciudadania</option>
                    <option value="Registro Civil">Registro Civil</option>
                    <option value="Tarjeta de Identidad">Tarjeta de Identidad</option>
                    <option value="Cedula de Extranjeria">Cedula de Extranjeria</option>
                    <option value="Pasaporte">Pasaporte</option>
                    <option value="Otro">Otro</option>
                </select>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-12">
                <label><strong>Numero Documento</strong></label>
                <input style="border: black; border-width: 1px; border-style: solid;" type="number" class="form-control" id="NumDocumentoPasaporte" name="NumDocumentoPasaporte" placeholder="Digite su numero de documento"  />
            </div>

            <div class="col-xs-6 col-sm-6 col-md-6" style="right:inherit">

                <button type="button" id="boton" class="btn btn-block btn-outline-primary btn-rounded btn-outline mt-4" onclick="CargarInfoinicial()" disabled>Buscar</button>
                @*<button type="button" class="btn btn-block btn-outline-primary btn-rounded btn-outline mt-4" onclick="Alternar(ConsultaPasaporte)">Buscar</button>*@

            </div>
            


        </div>


    </form>

</div>
<br />
